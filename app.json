{
  "expo": {
    "name": "Rick and Morty",
    "slug": "rick-and-morty",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#24282F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#24282F"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Aplicativo que nos mostre todos os personagens da Série Rick and Morty além de suas localizações e episódios.",
    "githubUrl": "https://github.com/yucorrea/desafio-mobile-superlogica"
  }
}
